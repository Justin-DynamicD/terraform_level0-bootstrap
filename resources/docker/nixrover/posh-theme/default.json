{
    "final_space": false,
    "console_title": true,
    "console_title_style": "template",
    "console_title_template": "{{if .Root}}(Admin){{end}} {{.Path}}",
    "blocks": [
        {
            "type": "newline",
            "alignment": "",
            "horizontal_offset": 0,
            "vertical_offset": 0,
            "segments": null
        },
        {
            "type": "prompt",
            "alignment": "left",
            "horizontal_offset": 0,
            "vertical_offset": 0,
            "segments": [
                {
                    "type": "root",
                    "style": "plain",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#BF616A",
                    "background": "",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "prefix": "",
                        "root_icon": ""
                    }
                },
                {
                    "type": "path",
                    "style": "plain",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#81A1C1",
                    "background": "",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "prefix": "",
                        "style": "full"
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "left",
            "horizontal_offset": 0,
            "vertical_offset": 0,
            "segments": [
                {
                    "type": "git",
                    "style": "plain",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#6C6C6C",
                    "background": "",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "branch_ahead_icon": "<#88C0D0>⇡ </>",
                        "branch_behind_icon": "<#88C0D0>⇣ </>",
                        "branch_icon": "",
                        "display_stash_count": true,
                        "display_status": true,
                        "display_status_detail": true,
                        "display_upstream_icon": true,
                        "github_icon": "",
                        "local_working_icon": "<#FFAFD7>*</>"
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "left",
            "horizontal_offset": 0,
            "vertical_offset": 0,
            "segments": [
                {
                    "type": "executiontime",
                    "style": "plain",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#A3BE8C",
                    "background": "",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "style": "austin"
                    }
                }
            ]
        },
        {
            "type": "newline",
            "alignment": "",
            "horizontal_offset": 0,
            "vertical_offset": 0,
            "segments": null
        },
        {
            "type": "prompt",
            "alignment": "left",
            "horizontal_offset": 0,
            "vertical_offset": 0,
            "segments": [
                {
                    "type": "exit",
                    "style": "plain",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#B48EAD",
                    "background": "",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "always_enabled": true,
                        "display_exit_code": false,
                        "error_color": "#BF616A",
                        "prefix": "❯"
                    }
                }
            ]
        }
    ]
}